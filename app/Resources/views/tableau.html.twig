{% block CollectionType_row %}
    <div class="col-md-3">
        {{ form_label(form,{'attr': {'class': 'form-control TVA'}}) }}
        {{ form_errors(form,{'attr': {'class': 'form-control TVA'}}) }}
        {{ form_widget(name,{'attr': {'class': 'form-control TVA'}}) }}
    </div>
{% endblock %}

{% block CollectionType_widget %}

    {{ form_widget(form) }}
    <br/>
    <p class="text-center">
        <a href="#" class="collection-up btn btn-default">&lt;</a>
        <a href="#" class="collection-remove btn btn-default">-</a>
        <a href="#" class="collection-add btn btn-default">+</a>
        <a href="#" class="collection-down btn btn-default">&gt;</a>
    </p>

{% endblock %}